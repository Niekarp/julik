{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    // "declaration": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "typeAcquisition": {
    "include": [
        "jquery",
    ]
  }
}
